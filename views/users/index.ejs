<h2>Users Table</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Zip Code</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.name.first + ' ' + user.name.last %></td>
                <td><%= user.email %></td>
                <td><%= user.zipCode %></td>
            </tr>
        <% }) %>
    </tbody>
</table>